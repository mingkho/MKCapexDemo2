  "private": true,
  "dependencies": {
    "@sap-cloud-sdk/http-client": "^3.5.0",
    "@sap-cloud-sdk/util": "^3.5.0",
    "@sap/cds": "^7",
    "@sap/cds-hana": "^2",
    "@sap/low-code-event-handler": "^1.2.3",
    "@sap/xsenv": ">=3.3.2 <3.3.3",
